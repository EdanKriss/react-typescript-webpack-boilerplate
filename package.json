{
  "name": "react-typescript-webpack-boilerplate",
  "version": "0.2.0",
  "description": "A React.js starter project. Uses webpack and ts-loader for transpilation, without babel.",
  "private": true,
  "main": "index.js",
  "repository": "",
  "scripts": {
    "dev": "webpack serve --mode development --env API_HOST=http://example",
    "build": "rm -rf ./dist && webpack --mode production --env API_HOST=https://example",
    "build-cdn": "rm -rf ./dist && webpack --mode production --env API_HOST= --env ASSET_PATH="
  },
